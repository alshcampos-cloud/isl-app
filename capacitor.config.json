{
  "appId": "ai.interviewanswers.app",
  "appName": "InterviewAnswers",
  "webDir": "dist",
  "server": {
    "hostname": "interviewanswers.ai",
    "androidScheme": "https",
    "iosScheme": "https"
  },
  "ios": {
    "contentInset": "always",
    "scrollEnabled": true,
    "backgroundColor": "#1e1b4b"
  },
  "android": {
    "backgroundColor": "#0f172a"
  },
  "plugins": {
    "SplashScreen": {
      "launchAutoHide": false,
      "backgroundColor": "#1e1b4b",
      "showSpinner": false,
      "androidScaleType": "CENTER_CROP",
      "splashFullScreen": true,
      "splashImmersive": true,
      "fadeOutDuration": 300
    },
    "StatusBar": {
      "style": "DARK",
      "backgroundColor": "#1e1b4b"
    }
  }
}
